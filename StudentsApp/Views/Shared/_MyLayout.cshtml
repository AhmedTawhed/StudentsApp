<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no" />
    <title>@ViewData["Title"] StudentsApp </title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/StudentsApp.styles.css" asp-append-version="true" />
    <link href="https://cdn.datatables.net/v/dt/dt-1.13.6/b-2.4.2/b-html5-2.4.2/fc-4.3.0/sc-2.2.0/sb-1.6.0/sp-2.2.0/sl-1.7.0/datatables.min.css" rel="stylesheet">
    <link type="text/css" href="~/lib/Scripts-For-Template/css/swipe.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/sweetalert2.css" />

</head>
<body>
    <header class="header-global" id="home">
        <partial name="_NavbarPartial" />
    </header>
    <br />
    <br />
    <br />


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <br />
    <br />
    <footer class="footer py-5 pt-lg-6">
        <partial name="_FooterPartial" />
    </footer>


    <!-- Core -->
    <script src="~/lib/Scripts-For-Template/vendor/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/lib/Scripts-For-Template/vendor/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/lib/Scripts-For-Template/vendor/headroom.js/dist/headroom.min.js"></script>

    <!-- Vendor JS -->
    <script src="~/lib/Scripts-For-Template/vendor/onscreen/dist/on-screen.umd.min.js"></script>
    <script src="~/lib/Scripts-For-Template/vendor/smooth-scroll/dist/smooth-scroll.polyfills.min.js"></script>

    <script async defer src="https://buttons.github.io/buttons.js"></script>

    <!-- Swipe JS -->
    <script src="~/lib/Scripts-For-Template/assets/js/swipe.js"></script>

    <!-- Sweet alert -->
    <script src="~/js/sweetalert2.js"></script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    <script src="https://cdn.datatables.net/v/dt/dt-1.13.6/b-2.4.2/b-html5-2.4.2/fc-4.3.0/sc-2.2.0/sb-1.6.0/sp-2.2.0/sl-1.7.0/datatables.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
